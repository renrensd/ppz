<!DOCTYPE module SYSTEM "module.dtd">

<module name="mag_cali" dir="mag_cali">
  <doc>
    <description>
      Calibration of magnetometer
    </description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings name="mag_cali">
        <dl_setting min="0" max="1" step="1" values="OFF|ON" var="mag_cali.manual_enable" module="mag_cali/mag_cali" shortname="Enable"/>
        <dl_setting min="0" max="1" step="1" values="DISABLE|ENABLE" var="mag_cali.need_cali" module="mag_cali/mag_cali" shortname="Enable"/>
      </dl_settings>
    </dl_settings>
  </settings>
  <header>
    <file name="mag_cali.h"/>
  </header>
  <init fun="mag_cali_init()"/>
  <periodic fun="mag_cali_periodic()" freq="50"/>
  <event fun="mag_cali_event()"/>
  <makefile target="ap|nps">
    <file name="mag_cali.c"/>
  </makefile>
</module>
