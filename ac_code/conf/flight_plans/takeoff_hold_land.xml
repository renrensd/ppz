<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="17.5" ground_alt="16" lat0="22.540656" lon0="113.923446" max_dist_from_home="10000" name="Rotorcraft Basic (Enac)" security_height="1">
  <header>
#include "autopilot.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0" y="0"/>
    <waypoint name="S1" x="0" y="0"/>
    <waypoint name="S2" x="0" y="0"/>
    <waypoint name="S3" x="0" y="0"/>
    <waypoint name="S4" x="0" y="0"/>
    <waypoint name="O1" x="-59.2" y="28.0"/>
    <waypoint name="O2" x="26.9" y="248.2"/>
    <waypoint name="O3" x="-82.7" y="16.0"/>
    <waypoint name="O4" x="111.6" y="242.6"/>
    <waypoint name="O5" x="110.4" y="323.1"/>
    <waypoint name="From" x="0" y="20"/>
    <waypoint name="Next" x="113.0" y="380.3"/>
    <waypoint name="P1" x="-104.7" y="377.4"/>
    <waypoint name="P2" x="-150" y="20"/>
    <waypoint name="P3" x="132.8" y="125.8"/>
    <waypoint name="P4" x="201.7" y="274.5"/>
    <waypoint name="P5" x="47.6" y="302.2"/>
    <waypoint name="P6" x="177.0" y="390.2"/>
    <waypoint name="P7" x="26.8" y="443.7"/>
  </waypoints>
  <blocks>
    <block name="circle">
      <circle radius="7" wp="O1"/>
    </block>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
      <circle radius="7" wp="O1"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Start Engine">
      <call fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
