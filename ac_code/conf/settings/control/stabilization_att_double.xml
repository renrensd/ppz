<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings target="ap|nps">
  <dl_settings>
    <dl_settings NAME="Att Loop">
      <dl_setting var="stabilization_gains.p.x" min="0" step="0.1" max="10"   module="stabilization/stabilization_attitude" shortname="p x"/>
			<dl_setting var="stabilization_gains.p.y" min="0" step="0.1" max="10"   module="stabilization/stabilization_attitude" shortname="p y"/>
			<dl_setting var="stabilization_gains.p.z" min="0" step="0.1" max="20"   module="stabilization/stabilization_attitude" shortname="p z"/>
			
			<dl_setting var="stabilization_gains.d.x" min="0" step="0.1" max="10"   module="stabilization/stabilization_attitude" shortname="d x"/>
			<dl_setting var="stabilization_gains.d.y" min="0" step="0.1" max="10"   module="stabilization/stabilization_attitude" shortname="d y"/>
			<dl_setting var="stabilization_gains.d.z" min="0" step="0.1" max="20"   module="stabilization/stabilization_attitude" shortname="d z"/>

			<dl_setting var="stabilization_gains.p_rate.x" min="0" step="10" max="3000"   module="stabilization/stabilization_attitude" shortname="p_rate x"/>
			<dl_setting var="stabilization_gains.p_rate.y" min="0" step="10" max="3000"   module="stabilization/stabilization_attitude" shortname="p_rate y"/>
			<dl_setting var="stabilization_gains.p_rate.z" min="0" step="10" max="3000"   module="stabilization/stabilization_attitude" shortname="p_rate z"/>

      <dl_setting var="stabilization_gains.i_rate.x" min="0" step="10" max="2000"    module="stabilization/stabilization_attitude" shortname="i_rate x"/>
      <dl_setting var="stabilization_gains.i_rate.y" min="0"  step="10" max="2000"    module="stabilization/stabilization_attitude" shortname="i_rate y"/>
			<dl_setting var="stabilization_gains.i_rate.z" min="0"  step="10" max="2000"    module="stabilization/stabilization_attitude" shortname="i_rate z"/>

      <dl_setting var="stabilization_gains.d_rate.x" min="0" step="1" max="500"   module="stabilization/stabilization_attitude" shortname="d_rate_x"/>                      
      <dl_setting var="stabilization_gains.d_rate.y" min="0" step="1" max="500"   module="stabilization/stabilization_attitude" shortname="d_rate_y"/>                  
      <dl_setting var="stabilization_gains.d_rate.z" min="0" step="1" max="500"   module="stabilization/stabilization_attitude" shortname="d_rate_z"/>        
    </dl_settings>
  </dl_settings>
</settings>
