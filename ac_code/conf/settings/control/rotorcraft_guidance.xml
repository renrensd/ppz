<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings target="ap|nps">
  <dl_settings>

    <dl_settings NAME="Vert Loop">    
      <dl_setting var="guid_v.pid_loop_mode_gcs" min="0" step="1" max="2" module="guidance/guidance_v" shortname="pid_mode" values="ACC|ACC_SPEED|ACC_SPEED_POS" persistent="true"/>
      <dl_setting var="guid_v.acc_filter_fc"    min="1" step="1" max="25"     module="guidance/guidance_v" shortname="acc_flt" handler="SetAccCutoff" />       
      <dl_setting var="guid_v.acc_z_pid.Kp"    min="0" step="0.01" max="5"     module="guidance/guidance_v" shortname="acc_z_Kp" />   
      <dl_setting var="guid_v.acc_z_pid.Ki"    min="0" step="0.01" max="5"     module="guidance/guidance_v" shortname="acc_z_Ki" />
      <dl_setting var="guid_v.speed_z_pid.Kp"    min="0" step="0.01" max="20"     module="guidance/guidance_v" shortname="speed_z_Kp" />
      <dl_setting var="guid_v.speed_z_pid.Ki"    min="0" step="0.01" max="20"     module="guidance/guidance_v" shortname="speed_z_Ki" />
      <dl_setting var="guid_v.speed_z_pid.Kd"    min="0" step="0.01" max="20"     module="guidance/guidance_v" shortname="speed_z_Kd" />
      <dl_setting var="guid_v.pos_z_pid.Kp"    min="0" step="0.01" max="5"     module="guidance/guidance_v" shortname="pos_z_Kp" />
      <dl_setting var="guid_v.pos_z_pid.Ki"    min="0" step="0.01" max="5"     module="guidance/guidance_v" shortname="pos_z_Ki" />
      <dl_setting var="guid_v.pos_z_pid.Kd"    min="0" step="0.01" max="5"     module="guidance/guidance_v" shortname="pos_z_Kd" /> 
    </dl_settings>

  <!--dl_settings name="Direction  Value">
   <dl_setting var="change_angel" min="0" step="0.1" max="45"   module="guidance/guidance_h" shortname="angel" param="GUIDANCE_C_AN"/>
   <dl_setting var="change_able" min="0" step="0.01" max="1"   module="guidance/guidance_h" shortname="able" param="GUIDANCE_C_AB"/>
   <dl_setting var="change_distance" min="1000" step="100" max="5000"   module="guidance/guidance_h" shortname="distance" param="GUIDANCE_C_DI"/>
   </dl_settings-->
   
   <!--dl_settings name="Imu_fre">
   	 <dl_setting var="acc_cutoff_fre" min="15" step="1" max="50"   module="subsystems/imu" shortname="acc" />
  	 <dl_setting var="gyro_cutoff_fre" min="15" step="1" max="50"   module="subsystems/imu" shortname="gyro" />
   </dl_settings-->

   <dl_settings name="Ins_Debug_Noise">
   	 <dl_setting var="acc_noise_debug" min="0.001" step="0.001" max="100.0"   module="subsystems/ins/ins_int" shortname="acc_noise" />
  	 <dl_setting var="gps_noise_debug" min="0.00001" step="0.00001" max="10.0"   module="subsystems/ins/vf_extended_float" shortname="gps_noise" />
   </dl_settings>
            
    <dl_settings NAME="Horiz Loop">
      <dl_setting var="guidance_h.pid_loop_mode_gcs" min="0" step="1" max="1" module="guidance/guidance_h" shortname="pid_mode" values="VEL|POS_VEL"/>
      <dl_setting var="guidance_h.ned_acc_filter_fc"    min="1" step="0.5" max="20" module="guidance/guidance_h" shortname="ned_acc_fc" handler="SetNedAccFc"/>
      <dl_setting var="guidance_h.ned_vel_filter_fc"    min="1" step="0.5" max="20" module="guidance/guidance_h" shortname="ned_vel_fc" handler="SetNedVelFc"/>
      <dl_setting var="guidance_h.vel_x_pid.Kp"    min="0" step="0.01" max="5" module="guidance/guidance_h" shortname="vel_Kp" handler="SetVelKp"/>
      <dl_setting var="guidance_h.vel_x_pid.Ki"    min="0" step="0.01" max="5" module="guidance/guidance_h" shortname="vel_Ki" handler="SetVelKi"/>
      <dl_setting var="guidance_h.vel_x_pid.Kd"    min="0" step="0.01" max="5" module="guidance/guidance_h" shortname="vel_Kd" handler="SetVelKd"/>
      <dl_setting var="guidance_h.pos_x_pid.Kp"    min="0" step="0.01" max="5" module="guidance/guidance_h" shortname="pos_Kp" handler="SetPosKp"/>
      <dl_setting var="guidance_h.pos_x_pid.Ki"    min="0" step="0.01" max="5" module="guidance/guidance_h" shortname="pos_Ki" handler="SetPosKi"/>
      <dl_setting var="guidance_h.pos_x_pid.Kd"    min="0" step="0.01" max="5" module="guidance/guidance_h" shortname="pos_Kd" handler="SetPosKd"/>
      <dl_setting var="guidance_h.NED_xy_speed_filter_fc"    min="1" step="1" max="25"     module="guidance/guidance_h" shortname="speed_flt" handler="SetSpeedCutoff"/>
      <dl_setting var="guidance_h.use_ref" min="0" step="1" max="1" module="guidance/guidance_h" shortname="use_ref" values="FALSE|TRUE" handler="SetUseRef" param="GUIDANCE_H_USE_REF" persistent="true"/>
      <dl_setting var="gh_ref.max_speed" min="0.1" step="0.1" max="15.0" module="guidance/guidance_h" shortname="max_speed" handler="SetMaxSpeed" param="GUIDANCE_H_REF_MAX_SPEED" type="float" persistent="true"/>
      <dl_setting var="guidance_h.approx_force_by_thrust" min="0" step="1" max="1" module="guidance/guidance_h" shortname="approx_force" values="FALSE|TRUE" param="GUIDANCE_H_APPROX_FORCE_BY_THRUST" type="uint8" persistent="true"/>
      <dl_setting var="gh_ref.tau" min="0.1" step="0.1" max="1.0" module="guidance/guidance_h" shortname="tau" handler="SetTau" param="GUIDANCE_H_REF_TAU" type="float" persistent="true"/>
      <dl_setting var="gh_ref.omega" min="0.1" step="0.1" max="3.0" module="guidance/guidance_h" shortname="omega" handler="SetOmega" param="GUIDANCE_H_REF_OMEGA" type="float" persistent="true"/>
      <dl_setting var="gh_ref.zeta" min="0.1" step="0.05" max="1.0" module="guidance/guidance_h" shortname="zeta" handler="SetZeta" param="GUIDANCE_H_REF_ZETA" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains.p" min="0" step="1" max="400" module="guidance/guidance_h" shortname="kp" param="GUIDANCE_H_PGAIN" type="int32" persistent="true"/>
      <dl_setting var="guidance_h.gains.d" min="0" step="1" max="400" module="guidance/guidance_h" shortname="kd" param="GUIDANCE_H_DGAIN" type="int32" persistent="true"/>
      <dl_setting var="guidance_h.gains.i" min="0" step="1" max="400" module="guidance/guidance_h" shortname="ki" handler="set_igain" param="GUIDANCE_H_IGAIN" type="int32" persistent="true"/>
      <dl_setting var="guidance_h.gains.v" min="0" step="1" max="400" module="guidance/guidance_h" shortname="kv" param="GUIDANCE_H_VGAIN" type="int32" persistent="true"/>
      <dl_setting var="guidance_h.gains.a" min="0" step="1" max="400" module="guidance/guidance_h" shortname="ka" param="GUIDANCE_H_AGAIN" type="int32" persistent="true"/>
      <!--dl_setting var="guidance_h.gains_f.p" min="0" step="0.01" max="50" module="guidance/guidance_h" shortname="fkp" param="GUIDANCE_H_FPGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.i" min="0" step="0.01" max="50" module="guidance/guidance_h" shortname="fki" param="GUIDANCE_H_FIGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.d" min="0" step="0.01" max="50" module="guidance/guidance_h" shortname="fkd" param="GUIDANCE_H_FDGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.in_p" min="0" step="0.01" max="200" module="guidance/guidance_h" shortname="fikp" param="GUIDANCE_H_FIPGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.in_i" min="0" step="0.01" max="50" module="guidance/guidance_h" shortname="fiki" param="GUIDANCE_H_FIIGAIN" type="float" persistent="true"/>
      <dl_setting var="guidance_h.gains_f.in_d" min="0" step="0.01" max="50" module="guidance/guidance_h" shortname="fikd" param="GUIDANCE_H_FIDGAIN" type="float" persistent="true"/-->
      <dl_setting var="hh"             min="0" step="500" max="1000000"   module="guidance/guidance_v" shortname="H_h"  param="GUIDANCE_H_TD_H" persistent="true"/>
      <dl_setting var="hh0"            min="0" step="500" max="1000000"   module="guidance/guidance_v" shortname="H_h0"  param="GUIDANCE_H_TD_H0" persistent="true"/>
      <dl_setting var="r_h"            min="0" step="10" max="2000"   module="guidance/guidance_v" shortname="H_r"  param="GUIDANCE_H_TD_R" persistent="true"/>
      <dl_setting var="guidance_h.sp.pos.x" MIN="-10" MAX="10" STEP="1"  module="guidance/guidance_h" shortname="sp_x_ned" unit="1/2^8m" alt_unit="m" alt_unit_coef="0.00390625"/>
      <dl_setting var="guidance_h.sp.pos.y" MIN="-10" MAX="10" STEP="1"  module="guidance/guidance_h" shortname="sp_y_ned" unit="1/2^8m" alt_unit="m" alt_unit_coef="0.00390625"/>
    </dl_settings>

    <dl_settings NAME="NAV">
      <dl_setting var="flight_altitude" MIN="0" STEP="0.1" MAX="400" module="navigation" unit="m" handler="SetFlightAltitude"/>
      <dl_setting var="nav_heading" MIN="0" STEP="1" MAX="360" module="navigation" unit="1/2^12r" alt_unit="deg" alt_unit_coef="0.0139882"/>
      <dl_setting var="nav_radius" MIN="-50" STEP="0.1" MAX="50" module="navigation" unit="m"/>
      <dl_setting var="nav_climb_vspeed" MIN="0" STEP="0.1" MAX="10.0" module="navigation" unit="m/s" param="NAV_CLIMB_VSPEED"/>
      <dl_setting var="nav_descend_vspeed" MIN="-10.0" STEP="0.1" MAX="0.0" module="navigation" unit="m/s" param="NAV_DESCEND_VSPEED"/>
      <dl_setting var="smooth_brake_accel" MIN="0.5" STEP="0.1" MAX="5.0" module="navigation" />
      <dl_setting var="pause_brake_accel" MIN="0.5" STEP="0.1" MAX="5.0" module="navigation" />
      <dl_setting var="set_carrot_angle"  min="30" step="1" max="90" module="navigation" shortname="carrot_angle" />
    </dl_settings>

  </dl_settings>
</settings>
